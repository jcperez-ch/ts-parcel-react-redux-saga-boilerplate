{
    "defaultSeverity": "error",
    "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
      "function-name": false,
      "import-name": false,
      "jsx-no-lambda": false,
      "no-consecutive-blank-lines": [true, 1],
      "object-shorthand-properties-first": false,
      "quotemark": [true, "single", "avoid-template", "jsx-double"],
      "semicolon": [true, "always"],
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "exports": "always",
            "functions": "never",
            "imports": "always",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ],
      "variable-name": false
    },
    "rulesDirectory": []
  }